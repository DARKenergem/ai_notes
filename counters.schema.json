{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Counters",
  "type": "object",
  "properties": {
    "_id": {"type": "string", "description": "Identifier for the counter, e.g., 'note_id'"},
    "seq": {"type": "integer", "description": "Current sequence value for generating unique IDs"}
  },
  "required": ["_id", "seq"]
} 